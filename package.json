{
  "name": "mas",
  "version": "1.0.0",
  "description": "The \"_s\" for Timber: a dead-simple theme that you can build from. The primary purpose of this theme is to provide a file structure rather than a framework for markup or styles. Configure your Sass, scripts, and task runners however you would like!",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "patternlab build --config ./patternlab-config.json",
    "build:uikit": "cross-env-shell PL_CONFIG_PATH='${INIT_CWD}/.patternlabrc.js' npm run build --prefix node_modules/@pattern-lab/uikit-workshop -- --patternlabrc '$PL_CONFIG_PATH'",
    "dev": "node ./node_modules/@pattern-lab/uikit-workshop/build-tools.js",
    "help": "patternlab --help",
    "install": "patternlab install --config ./patternlab-config.json",
    "serve": "patternlab serve --config ./patternlab-config.json",
    "start": "npm run serve",
    "version": "patternlab --version",
    "patternlab": "patternlab",
    "gulp": "gulp -- "
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@pattern-lab/cli": "^5.15.0",
    "@pattern-lab/core": "^5.15.0",
    "@pattern-lab/engine-twig-php": "^5.15.0",
    "@pattern-lab/uikit-workshop": "^5.15.0"
  },
  "devDependencies": {
    "browser-sync": "^2.27.5",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-merge-css": "^1.0.2",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "^5.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-string-replace": "^1.1.2",
    "gulp-uglify": "^3.0.2",
    "postcss-critical-split": "^2.5.3",
    "sass": "^1.42.1",
    "yargs": "^17.2.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "browserslist": [
    "> 5%",
    "IE 11"
  ]
}
